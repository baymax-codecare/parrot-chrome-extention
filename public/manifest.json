{
  "short_name": "Parrot Sniper",
  "name": "Parrot: Solana NFT Sniper",
  "version": "0.0.9",
  "manifest_version": 3,
  "description": "",
  "icons": { "192": "./logo192.png" },
  "permissions": [
    "storage",
    "tabs",
    "alarms",
    "webRequest",
    "notifications",
    "webNavigation",
    "activeTab",
    "gcm",
    "webRequest",
    "https://beta.api.solanalysis.com/graphql"
  ],
  "host_permissions": [
    "https://*.hyperspace.xyz/*",
    "http://*/*",
    "https://*/*",
    "https://hyperspace.xyz/*",
    "https://*.api.mixpanel.com/*",
    "https://api.mixpanel.com/*",
    "https://*.mixpanel.com/*"
  ],
  "background": {
    "service_worker": "static/js/background.js"
  },
  "content_scripts": [
    {
      "matches": ["https://*.hyperspace.xyz/*", "https://*.mixpanel.com/*"],
      "js": ["static/js/content.js"]
    }
  ],
  "action": { "default_title": "Mint", "default_popup": "index.html" },
  "content_security_policy": {
    "script-src": ["self", "unsafe-eval", "https://api.mixpanel.com"],
    "object-src": "self"
  }
}
